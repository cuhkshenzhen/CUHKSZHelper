{
	"manifest_version": 2,
	"name": "Moodle Helper",
	"author": "CJY@CUHKSZ",
	"version": "0.1.2",
	"content_scripts": [
		{
			"matches": ["https://elearning.cuhk.edu.cn/*"],
			"css": ["style.css"],
			"js": [
				"jquery.min.js",
				"helper.js",
				"spider.js",
				"displayer.js",
				"quiz.js",
				"others.js"
			],
			"all_frames": true
		}
	],
	"web_accessible_resources": [
		"index.template.html",
		"quiz.template.html",
		"ebp_hint.template.html"
	],
	"permissions": [
		"activeTab"
	]
}
