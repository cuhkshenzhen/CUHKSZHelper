{
	"manifest_version": 2,
	"name": "Moodle Helper",
	"version": "1.0",
	"content_scripts": [
		{
			"matches": ["https://elearning.cuhk.edu.cn/*"],
			"css": ["style.css"],
			"js": ["jquery.min.js", "helper.js", "spider.js", "displayer.js"],
			"all_frames": true
		}
	],
	"web_accessible_resources": ["index.template.html"],
	"permissions": [
		"activeTab"
	]
}
